{
  "name": "Chess Analaytics",
  "description": "Get good",
  "version": "1",
  "manifest_version": 2,
  "content_security_policy": "default-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://lichess.org/*"],
      "js": ["script.js", "./scripts/chess.js", "./scripts/chart.umd.js", "./scripts/chartjs-plugin-datalabels.min.js"],
      "web_accessible_resources": [
        "./scripts/chess.js",
        "./scripts/chart.umd.js",
        "./scripts/chart.umd.js.map",
        "view.html",
        "./scripts/chartjs-plugin-datalabels.min.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "./scripts/chess.js",
    "./scripts/chart.umd.js",
    "./scripts/chart.umd.js.map",
    "view.html",
    "./scripts/chartjs-plugin-datalabels.min.js"
  ],
  "permissions": ["<all_urls>"]
}
